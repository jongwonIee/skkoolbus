<% if @on == false %>
    <%= render 'popup' %>
<% end %>
<!-- shuttle_walk를 조금 더 짜보자 // 국제관에서 -> 다른 건물 // 농구장 -> 국제관 -->
<div class="container">
  <!-- Navigation_Top -->
  <nav class="navbar navbar-default navbar-fixed-top topnav" role="navigation">
    <div class="container topnav">
      <!-- Brand and toggle get grouped for better mobile display -->
      <div class="navbar-header">
        <div onclick="openNav()" class="nav_ic_left ">
          <%= image_tag 'skkbus_ic_info.png' %>
        </div>
        <a class="navbar-brand topnav" style="font-family: Nunito; font-weight: 100;" href="/main"><span style="font-family: Nunito; font-weight: bold; text-shadow: 0.6px 0px 0px">SKK</span>OOL BUS</a>
        <div class="nav_ic_right">
          <a href="/schedule"><%= image_tag 'skkbus_ic_schedule.png' %></a>
        </div>
      </div>
      <!-- /.navbar-collapse -->
    </div>
    <!-- /.container -->
  </nav>
</div>



<div class="bus_all" style="">

  <%= render 'info' %>

  <div class="bus_line_container">
    <div class="bus_line">
      <div class="bus_station_section1">

        <div class="s2 bus_dep1">
          <div class="bus_ic_dep">
          </div>
        </div>
        <div class="s1 bus_station_sequence1 clickable">
          <a data-toggle="modal" data-target="#Basketball_court">
            <%= image_tag 'skkbus_ic_left.png', class: 'sequence1_arrow' %>
          </a>
        </div>
        <div class="s2 bus_dep10">
          <div class="bus_ic_dep">
          </div>
        </div>
      </div>
      <div class="bus_station_section2">
        <a data-toggle="modal" data-target="#StudentHall">
          <div class="s1 bus_station_sequence2 clickable">
            <%= image_tag 'skkbus_ic_left.png' , class: 'sequence10_arrow' %>
            <div class="bus_ic_station">
            </div>
          </div>
        </a>
        <div class="s1 bus_station_sequence10 clickable">
          <a data-toggle="modal" data-target="#Sixhundred_Anniversary">
            <%= image_tag 'skkbus_ic_left.png' , class: 'sequence2_arrow' %>
            <div class="bus_ic_station">
            </div>
          </a>
        </div>
      </div>
      <div class="bus_station_section3">
        <div class="s2 bus_dep2">
          <div class="bus_ic_dep">
          </div>
        </div>
        <div class="s2 bus_dep9">
          <div class="bus_ic_dep">
          </div>
        </div>
      </div>
      <div class="bus_station_section4">
