            <%= image_tag 'skkbus_ic_left.png' , class: 'sequence10_arrow' %>
            <div class="bus_ic_station">
            </div>
          </a>
        </div>
        <div class="s1 bus_station_sequence9 clickable">
          <a data-toggle="modal" data-target="#Entrance">
            <%= image_tag 'skkbus_ic_left.png' , class: 'sequence2_arrow' %>
            <div class="bus_ic_station">
            </div>
          </a>
        </div>
      </div>
      <div class="bus_station_section5">
        <div class="s2 bus_dep3">
          <div class="bus_ic_dep">
          </div>
        </div>
        <div class="s2 bus_dep8">
          <div class="bus_ic_dep">
          </div>
        </div>
      </div>
      <div class="bus_station_section6">
        <div class="s1 bus_station_sequence4 clickable">
          <a data-toggle="modal" data-target="#HyehwaRottery">
            <%= image_tag 'skkbus_ic_left.png', class: 'sequence10_arrow' %>
            <div class="bus_ic_station">
            </div>
          </a>
        </div>
        <div class="s1 bus_station_sequence8 clickable">
          <a data-toggle="modal" data-target="#McDonald">
            <%= image_tag 'skkbus_ic_left.png' , class: 'sequence2_arrow' %>
            <div class="bus_ic_station">
            </div>
          </a>
        </div>
      </div>
      <div class="bus_station_section7">
        <div class="s2 bus_dep4">
          <div class="bus_ic_dep">
          </div>
        </div>
        <div class="s1 bus_station_sequence6 clickable">
          <a data-toggle="modal" data-target="#HyehwaStation">
            <%= image_tag 'skkbus_ic_left.png' , class: 'sequence6_arrow' %>
          </a>
        </div>
        <div class="s2 bus_dep6">
          <div class="bus_ic_dep">
          </div>
        </div>
      </div>
    </div>

  </div>
  <div class="bus_info_container">
    <div class="bus_info">
      <div class="bus_info_section1">
